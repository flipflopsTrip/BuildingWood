	//이미지 슬라이드
	function initReport() {
		let imgSlideW = 0;
		$('li', '.imgSlide').each(function() {
			imgSlideW += $(this).outerWidth(true);
		});
		let vw = $(".popReport dl.attach > dd").outerWidth(true);
		if (imgSlideW > vw) {
			if (!$('.imgSlide').hasClass('swiper-wrapper')) {
				$('.imgSlide').wrap('<div class="swiper-container"></div>').addClass('swiper-wrapper').children('li').addClass('swiper-slide');
				let imgSwiper = new Swiper('.attach .swiper-container', {
					autoplay: false,
					allowTouchMove: true,
					slidesPerView: 'auto',
					spaceBetween: 0,
					freeMode: true
				});
			} else {
				$('.swiper-wrapper').children('li').addClass('swiper-slide');
				let imgSwiper = document.querySelector('.attach .swiper-container').swiper;
				imgSwiper.update();
			}
		} else {
			if ($('.imgSlide').hasClass('swiper-wrapper')) {
				$('.imgSlide').unwrap();
				$('.attach .imgSlide').removeClass('swiper-wrapper').removeAttr('style');
				$('.attach .imgSlide li').removeAttr('class').removeAttr('style');
			}
		}
	}

	function saveUserReport() {

		
		if(document.getElementById("reportType").value == "") {
			alert("신고사유를 선택해주세요.");
			return;
		}

		if(document.getElementById("reportContents").value == "") {
			alert("신고내용을 입력해주세요.");
			return;
		}

		let formData = new FormData(document.reportForm);

		for(let i = 0; i < reportFilesToUpload.length; i++) {
			if(reportFilesToUpload[i].fileIndex === '') {
				formData.append('reportImageFile[]', reportFilesToUpload[i].file);
			}
		}

		// let spinHandle = loadingOverlay.activate();
		$.ajax({
			url : '/SaveUserReportAJX',
			data : formData,
			processData : false,
			contentType : false,
			type : 'post',
			headers: {
				'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
			},
			success : function(result) {
				if(result.result === "SUCCESS") {
					initPop("reportDone");
